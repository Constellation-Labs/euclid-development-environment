{
  "github_token": "",
  "metagraph_id": "",
  "tessellation_version": "2.3.1",
  "project_name": "",
  "framework": {
    "name": "currency",
    "modules": [
      "data"
    ],
    "version": "v2.3.1",
    "ref_type": "tag"
  },
  "p12_files": {
    "genesis": {
      "file_name": "token-key.p12",
      "alias": "token-key",
      "password": "password"
    },
    "validators": [
      {
        "file_name": "token-key-1.p12",
        "alias": "token-key-1",
        "password": "password"
      },
      {
        "file_name": "token-key-2.p12",
        "alias": "token-key-2",
        "password": "password"
      }
    ]
  },
  "docker": {
    "default_containers": [
      "global-l0",
      "metagraph-l0",
      "currency-l1",
      "data-l1",
      "monitoring"
    ]
  },
  "deploy": {
    "network": {
      "name": "integrationnet|mainnet",
      "gl0_node": {
        "ip": ":gl0_node_ip",
        "id": ":gl0_node_id",
        "public_port": ":gl0_node_public_port"
      }
    },
    "ansible": {
      "hosts": "infra/ansible/hosts.ansible.yml",
      "playbooks": {
        "deploy": "infra/ansible/playbooks/deploy/deploy.ansible.yml",
        "start": "infra/ansible/playbooks/start/start.ansible.yml"
      }
    }
  }
}
